<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Library">
    <!-- Main Background Color -->
    <Color x:Key="MainBackgroundColor">#f5f5f5</Color>
    <SolidColorBrush Color="{StaticResource MainBackgroundColor}" x:Key="MainBackgroundColorBrush" />

    <!-- dark Background Color -->
    <Color x:Key="DarkBackgroundColor">#e6e6e6</Color>
    <SolidColorBrush Color="{StaticResource DarkBackgroundColor}" x:Key="DarkBackgroundColorBrush" />

    <!-- Text BlacK Color -->
    <Color x:Key="TextBlackColor">#666666</Color>
    <SolidColorBrush Color="{StaticResource TextBlackColor}" x:Key="TextBlackColorBrush" />

    <!-- Text Green Color -->
    <Color x:Key="TextGreenColor">#39b54a</Color>
    <SolidColorBrush Color="{StaticResource TextGreenColor}" x:Key="TextGreenColorBrush" />
    
    <!-- Light Red-->
    <Color x:Key="LightRedColor">#ef497f</Color>
    <SolidColorBrush Color="{StaticResource LightRedColor}" x:Key="LightRedColorBruh" />

    <!-- blue color-->
    <Color x:Key="BlueColor">#54cde7</Color>
    <SolidColorBrush Color="{StaticResource BlueColor}" x:Key="BlueColorBruh" />
    
    <!-- dark light color -->
    <Color x:Key="TextLightDarkColor">#d5d5d5</Color>
    <SolidColorBrush x:Key="TextLightDarkColorBrush" Color="{StaticResource TextLightDarkColor}" />
    
    <!-- light black color-->
    <Color x:Key="LightBlackColor">#e6e6e6</Color>
    <SolidColorBrush x:Key="LightBlackColorBrush" Color="{StaticResource LightBlackColor}" />







</ResourceDictionary>